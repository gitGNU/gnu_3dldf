#### *** (3) Clean

clean-local: 
	$(MAKE) purge

#### *** (3) Purge

.PHONY: purge

purge:
	rm -f $(EPS_FILES)

#### *** (3) Implicit rules

.PRECIOUS : %.mp %.dvi %.ps

%.pdf : %.ps
	ps2pdf $<

%.ps : %.dvi
	dvips -o $@ $<

%.dvi : %.txt %.1
	tex $<

%.1 : %.mp
	mpost $<

%.mp : %.ldf
	$(top_builddir)/src/3dldf $<

